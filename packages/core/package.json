{
  "name": "@tf-engine/core",
  "version": "1.1.1",
  "description": "Transform Frame Engine â€“ a directed graph of spatial transforms between named reference frames",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --shims && mkdir -p dist/wasm && cp -r src/wasm/pkg dist/wasm/",
    "build:rust": "cd rust && wasm-pack build --target nodejs --out-dir ../src/wasm/pkg --out-name index",
    "prepublishOnly": "npm run build && npm test",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "bench": "vitest bench --run",
    "lint": "eslint src tests"
  },
  "keywords": [
    "transform",
    "frame",
    "robotics",
    "spatial",
    "graph",
    "quaternion"
  ],
  "author": "Francisco Rueda Esquivel <franruedaesq>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/franruedaesq/TF-engine/issues"
  },
  "homepage": "https://github.com/franruedaesq/TF-engine#readme",
  "dependencies": {
    "gl-matrix": "^3.4.4"
  },
  "devDependencies": {
    "@types/node": "^25.3.2",
    "@vitest/coverage-v8": "^4.0.18",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
