{
  "name": "@tf-engine/urdf-loader",
  "version": "1.0.0",
  "description": "URDF (Unified Robot Description Format) loader for @tf-engine/core â€“ hydrate a TFTree from a ROS URDF XML string",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "prepublishOnly": "npm run build && npm test",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src tests"
  },
  "keywords": [
    "urdf",
    "robot",
    "ros",
    "transform",
    "frame",
    "spatial"
  ],
  "author": "Francisco Rueda Esquivel <franruedaesq>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/franruedaesq/TF-engine/issues"
  },
  "homepage": "https://github.com/franruedaesq/TF-engine#readme",
  "dependencies": {
    "@tf-engine/core": "*"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.18",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
